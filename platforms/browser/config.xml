<?xml version='1.0' encoding='utf-8'?>
<widget id="com.bulag.appbulag" version="1.4.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <name>BrilliantUlag</name>
    <description>
        BrilliantUlag
    </description>
    <author email="sv_shulga@ukr.net" href="http://shulga.makels.com">
        Zerg Solutions
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <icon src="icon.png" />
    <icon density="mdpi" height="1024" platform="ios" src="res/ios/icon1024x1024.png" width="1024" />
    <icon density="mdpi" height="152" platform="ios" src="res/ios/icon152x152.png" width="152" />
    <icon density="mdpi" height="120" platform="ios" src="res/ios/icon120x120.png" width="120" />
    <icon density="mdpi" height="76" platform="ios" src="res/ios/icon76x76.png" width="76" />
    <config-file mode="replace" parent="NSPhotoLibraryUsageDescription" platform="ios">
        <string>We added new photos from camera for attached to orders.</string>
    </config-file>
</widget>
